{
  "name": "webpack-eliminate-loader",
  "version": "0.0.0",
  "main": "lib/webpack-eliminate-loader.cjs.js",
  "module": "lib/webpack-eliminate-loader.esm.js",
  "browser": "lib/webpack-eliminate-loader.umd.js",
  "typings": "lib/src/index.d.ts",
  "dependencies": {
    "loader-utils": "1.2.3",
    "schema-utils": "2.1.0",
    "webpack": "4.39.3"
  },
  "devDependencies": {
    "@types/loader-utils": "1.1.3",
    "@types/schema-utils": "1.0.0",
    "husky": "3.0.1",
    "np": "5.0.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "3.0.0",
    "rollup": "1.12.2",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "ts-node": "8.1.0",
    "tslib": "1.9.3",
    "typescript": "3.4.5"
  },
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "dev": "rollup -c -w",
    "test": "ts-node test/test.ts",
    "pretest": "npm run build",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "prepublishOnly": "np --no-cleanup --yolo --no-publish --any-branch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
